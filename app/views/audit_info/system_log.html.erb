<div class="row">
    <div class="col-lg-12">

        <h1 class="page-header bg-warning">
            System Log
        </h1>

        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Type</th>
                    <th>Hostname</th>
                    <th>OS Name</th>
                    <th>Version</th>
                    <th>Serial No</th>
                    <th>Model No</th>
                    <th>Scripted at</th>
                    <th>Uploaded at</th>
                </tr>
            </thead>
    
            <tbody>
                <% @sys_log.each do |sy| %>                                
                    <tr>
                        <td><%= sy.form_factor %></td>
                        <td><%= sy.hostname %>  </td>
                        <td><%= sy.os_name %>   </td>
                        <td><%= sy.os_version %></td>
                        <td><%= sy.serial %>    </td>
                        <td><%= sy.model %>     </td>
                        <td><%= sy.timestamp %> </td>
                        <td><%= sy.created_at %></td>
                    </tr>
                <% end %>
            </tbody>
        </table>
    </div>
</div>